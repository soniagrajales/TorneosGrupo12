@page
@model Frontend.Pages.DetailsModel
@{
    ViewData["Title"]="Details";
}
<h1>Detalles del Municipio</h1>

<div class="form-horizontal">
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <input type="hidden" asp-for="Municipio.Id" />
                <div class="form-group">
                    <label asp-for="Municipio.Id" class="control-label"> </label>
                    <input asp-for="Municipio.Id" class="form-control" readonly>
                </div>

                <div class="form-group">
                    <label asp-for="Municipio.Nombre" class="control-label"> </label>
                    <input asp-for="Municipio.Nombre" class="form-control" readonly>
                </div>                
            </form>
        </div>
    </div>
    <div>
        <a asp-page="MIndex">Ir al Inicio</a>
    </div>
</div>