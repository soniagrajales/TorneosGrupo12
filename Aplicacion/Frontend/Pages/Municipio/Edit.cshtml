@page
@model Frontend.Pages.EditModel
@{
    ViewData["Title"]="Edit";
}
<h1>Editar Municipio</h1>
<h2 class="text-danger">@ViewData["Error"] </h2>
<div class="form-horizontal">
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <input type="hidden" asp-for="Municipio.Id" />
                <div class="form-group">
                    <label asp-for="Municipio.Nombre" class="control-label"> </label>
                    <input asp-for="Municipio.Nombre" class="form-control">
                </div>
                <div  class="form-group">
                    <input type="submit" value="Editar" class="btn btn-primary">
                </div>
            </form>
        </div>
    </div>
    <div>
        <a asp-page="MIndex">Ir al Inicio</a>
    </div>
</div>
